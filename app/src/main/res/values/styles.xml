<resources>


	<style name = "AppTheme" parent = "Theme.AppCompat.Light.DarkActionBar">
	</style>

	<style name = "ShadowedText" parent = "Theme.AppCompat.Light.DarkActionBar">
		<item name = "android:textColor">#000000</item>
		<item name = "android:shadowColor">#FFFFFF</item>
		<item name = "android:shadowDx">1</item>
		<item name = "android:shadowDy">1</item>
		<item name = "android:shadowRadius">2</item>
	</style>

	<style name = "AppDark" parent = "Theme.AppCompat.Light.DarkActionBar">
		<item name = "android:background">@drawable/round_back_green_dark</item>
	</style>

	<style name = "AppDarkStroke" parent = "Theme.AppCompat.Light.DarkActionBar">
		<item name = "android:background">@drawable/round_back_green_dark_stroke</item>
	</style>

	<style name = "AppLight" parent = "Theme.AppCompat.Light.DarkActionBar">
		<item name = "android:background">@drawable/round_back_green_light</item>
	</style>

</resources>
